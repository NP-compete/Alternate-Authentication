{
  "name": "adriapp",
  "version": "1.0.0",
  "description": "AdriApp - Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/icon2.png --prune=true --out=dist --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"AdriApp\"",
    "package-mac": "electron-packager . --overwrite --asar=true --platform=darwin --arch=x64 --icon=src/assets/icons/icon3.icns --prune=true --out=dist --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"AdriApp\"",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/assets/icons/icon.ico --prune=true --out=dist --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"AdriApp\""
  },
  "repository": "https://github.com/adrianrosa/adriapp-electron",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.0.6"
  },
  "dependencies": {
    "electron-context-menu": "^0.9.1",
    "electron-packager": "^8.7.2",
    "electron-tabs": "^0.7.1"
  }
}
